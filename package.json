{
	"name": "webpack-eslint-config",
	"version": "1.0.0",
	"description": "Webpack and eslint config",
	"main": "index.tsx",
	"keywords": [
		"webpack",
		"eslint"
	],
	"author": "Vladislav Gaponov",
	"license": "ISC",
	"scripts": {
		"build": "NODE_ENV=production webpack",
		"dev": "NODE_ENV=development webpack",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.js'"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.18.9",
		"@babel/core": "^7.18.9",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@commitlint/cli": "^17.0.3",
		"babel-loader": "^8.2.5",
		"clean-webpack-plugin": "^4.0.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.1",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^4.7.4"
	}
}
